    "# Return Subsequences"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def subsequences(string):\n",
    "    \n",
    "    if len(string) == 0:\n",
    "        output = [\"\"]\n",
    "        return output\n",
    "    \n",
    "    smallOutput = subsequences(string[1:])\n",
    "    output = []\n",
    "    for s in smallOutput:\n",
    "        output.append(s)\n",
    "        output.append(string[0] + s)\n",
    "    return output\n",
    "\n",
    "string = input()\n",
    "ans = subsequences(string)\n",
    "for ele in ans:\n",
    "    print(ele)"
   ]
